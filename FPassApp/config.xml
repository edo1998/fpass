<?xml version='1.0' encoding='utf-8'?>
<widget id="com.fpass.app" version="1.0.1" versionCode="101" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>FPass</name>
    <description>
        Fpass, accedi ovunque.
    </description>
    <author email="info@rinonline.com" href="http://rinonline.com">
        Pentasistemi S.a.s
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="26" />
    <icon src="www/icon.png" />
    <platform name="android">
    </platform>
    <platform name="ios">
    </platform>
    <platform name="wp8">
    </platform>
    <platform name="windows">
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-device" spec="~1.1.6" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.7.1" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.3" />
    <plugin name="cordova-plugin-vibration" spec="~2.1.5" />
    <!--<plugin name="cordova-plugin-whitelist" spec="1.1.0" />-->
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access origin="*" />
    <engine name="android" spec="^6.3.0" />
    <engine name="ios" spec="^4.5.4" />
</widget>
