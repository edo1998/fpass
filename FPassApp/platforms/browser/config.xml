<?xml version='1.0' encoding='utf-8'?>
<widget id="com.fpass.app" version="1.0.1" versionCode="101" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>FPass</name>
    <description>
        Fpass, accedi ovunque.
    </description>
    <author email="info@rinonline.com" href="http://rinonline.com">
        Pentasistemi S.a.s
    </author>
    <content src="index.html" />
    <icon src="www/icon.png" />
    <allow-intent href="http://*/*" />
